{
    "projects": {
        "createProject": {
            "type": "Mutation",
            "description": "Create a new project",
            "usage": "createProject(projectInput: ProjectInput!)",
            "returns": "{ createProject: Project }",
            "exceptions": [
                {
                    "name": "MissingMandatoryFieldException",
                    "description": "One required project input field is missing",
                    "code": 2001
                }
            ]
        },
        "updateResolver": {
            "type": "Mutation",
            "description": "Update partially an existing project",
            "usage": "updateProject(projectInput: UpdateProjectInput!)",
            "returns": "{ updateProject: Project }",
            "exceptions": [
                {
                    "name": "ProjectNotFoundException",
                    "description": "The project id provided doesn't exist",
                    "code": 2002
                }
            ]
        },
        "findManyProjects": {
            "type": "Query",
            "description": "Find many projects",
            "usage": "findManyProjects()",
            "returns": "Project[]"
        },
        "findProjectById": {
            "type": "Query",
            "description": "Find one project",
            "usage": "findProjectById(id: String!)",
            "returns": "Project!",
            "exceptions": [
                {
                    "name": "ProjectNotFoundException",
                    "description": "The project id provided doesn't exist",
                    "code": 2002
                }
            ]
        }
    },
    "tasks": {
        "createTask": {
            "type": "Mutation",
            "description": "Create a new task",
            "usage": "createTask(taskInput: TaskInput!)",
            "returns": "{ createTask: Task }",
            "exceptions": [
                {
                    "name": "MissingMandatoryFieldException",
                    "description": "One required task input field is missing",
                    "code": 2001
                }
            ]
        },
        "selectOneTask": {
            "type": "Query",
            "description": "Select one task by id",
            "usage": "selectOneTask(id: String!)",
            "returns": "{ task: Task }"
        },
        "assignTaskStatus": {
            "type": "Mutation",
            "description": "Assign a task status to a task",
            "usage": "assignTaskStatus(taskId: String!, taskStatusId: String!)",
            "returns": "{ assignTaskStatus: Task }",
            "exceptions": [
                {
                    "name": "UnknownTaskException",
                    "description": "The task doesn't exist",
                    "code": 4001
                },
                {
                    "name": "UnknownTaskStatusException",
                    "description": "The task status doesn't exist",
                    "code": 3002
                }
            ]
        }
    },
    "taskStatus": {
        "createTaskStatus": {
            "type": "Mutation",
            "description": "Create new task status",
            "usage": "createTaskStatus(taskStatusInput: TaskStatusInput!)",
            "returns": "{ createTaskStatus: TaskStatus }",
            "exceptions": [
                {
                    "name": "AlreadyExistsException",
                    "description": "This task status already exists",
                    "code": 3001
                }
            ]
        }
    },
    "sprint": {
        "createSprint": {
            "type": "Mutation",
            "description": "Create a sprint onto an existing project",
            "usage": "createSprint(sprintInput: SprintInput!)",
            "returns": "{ createSprint: Sprint }",
            "exceptions": [
                {
                    "name": "InvalidDateFormatException",
                    "description": "The date format entered by the user is not valid",
                    "code": 2002
                }
            ]
        }
    }
}

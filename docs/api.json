{
    "projects": {
        "createProject": {
            "type": "Mutation",
            "description": "Create a new project",
            "usage": "createProject(projectInput: ProjectInput!)",
            "returns": "{ createProject: Project }",
            "exceptions": [
                {
                    "name": "MissingMandatoryFieldException",
                    "description": "One required project input field is missing",
                    "code": 2001
                }
            ]
        },
        "findManyProjects": {
            "type": "Query",
            "description": "Find many projects",
            "usage": "findManyProjects()",
            "returns": "Project[]"
        },
        "findOneProject": {
            "type": "Query",
            "description": "Find one projects",
            "usage": "findProjectById(id: String!)",
            "returns": "Project!",
            "exceptions": [
                {
                    "name": "ProjectNotFoundException",
                    "description": "One required project input field is missing",
                    "code": 2002
                }
            ]
        }
    },
    "tasks": {},
    "sprint": {}
}

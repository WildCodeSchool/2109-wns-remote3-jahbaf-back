{
    "projects": {
        "createProject": {
            "type": "Mutation",
            "description": "Create a new project",
            "usage": "createProject(projectInput: ProjectInput!)",
            "returns": "{ createProject: Project }",
            "exceptions": [
                {
                    "name": "MissingMandatoryFieldException",
                    "description": "One required project input field is missing",
                    "code": 2001
                }
            ]
        }
    },
    "tasks": {},
    "taskStatus": {
        "createTaskStatus": {
            "type": "Mutation",
            "description": "Create new task status",
            "usage": "createTaskStatus(taskStatusInput: TaskStatusInput!)",
            "returns": "{ createTaskStatus: TaskStatus }",
            "exceptions": [
                {
                    "name": "AlreadyExistsException",
                    "description": "This task status already exists",
                    "code": 3001
                }
            ]
        }
    },
    "sprint": {}
}
